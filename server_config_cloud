listen 80;

root /usr/src/projects/;
threads 4;

handler static {
	location /static;
	root static_src;
}

handler echo {
	location /echo;
}

handler status {
	location /status;
}

handler proxy {
	location /ucla;
	remote_url www.ucla.edu;
	remote_port 80;
}

handler 404 {
	location /;
}

handler meme {
	location /meme;
	root meme_src;
}